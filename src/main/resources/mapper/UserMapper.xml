<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.swpu.funchat.web.dao.UserDao">

    <select id="getUserInfoById" resultType="UserInfo">
        select * from user_info where user_id = #{id}
    </select>

    <select id="getUserAuthByUsername" resultType="UserAuth">
        select * from user_auth where identifier = #{username}
    </select>
</mapper>